  <DataSets>
    <DataSet Name="Godina">
      <Query>
        <DataSourceName>ComputerStoreDW</DataSourceName>
        <CommandText>SELECT DISTINCT CONVERT(nvarchar(255),Godina) 
FROM DimVrijeme
ORDER BY CONVERT(nvarchar(255),Godina)</CommandText>
      </Query>
      <Fields>
        <Field Name="Godina">
          <DataField />
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Godina.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Fields!Godina.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>ComputerStoreDW</DataSourceName>
        <CommandText>SELECT DP.ImePrezime, SUM(FP.Kolicina) as UkupnoProdano, SUM(FP.Zarada) as Zarada
FROM FactProdaja AS FP INNER JOIN DimProdavaci AS DP
ON FP.ProdavacKey = DP.ProdavacKey INNER JOIN
DimVrijeme AS DV ON DV.VrijemeKey=FP.VrijemeKey
INNER JOIN DimProizvodi AS DPR ON FP.ProizvodKey = DPR.ProizvodKey
GROUP BY DV.Godina,DP.ImePrezime,fp.NacinPlacanja
HAVING SUM(FP.Kolicina)&gt;0
ORDER BY DV.Godina asc</CommandText>
      </Query>
      <Fields>
        <Field Name="ImePrezime">
          <DataField>ImePrezime</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UkupnoProdano">
          <DataField>UkupnoProdano</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Zarada">
          <DataField>Zarada</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="NacinPlacanja">
      <Query>
        <DataSourceName>ComputerStoreDW</DataSourceName>
        <CommandText>SELECT NULL
UNION
SELECT DISTINCT NacinPlacanja FROM FactProdaja</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!ID.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Fields!ID.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
