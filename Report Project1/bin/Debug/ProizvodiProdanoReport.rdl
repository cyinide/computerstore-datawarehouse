        </Field>
        <Field Name="Prodano">
          <DataField>Prodano</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Kanton">
      <Query>
        <DataSourceName>ComputerStoreDW</DataSourceName>
        <CommandText>Select DISTINCT Kanton
from DimLokacije
WHERE Kanton NOT LIKE 'Nepoznato'</CommandText>
      </Query>
      <Fields>
        <Field Name="Kanton">
