  <DataSets>
    <DataSet Name="MainDataset">
      <Query>
        <DataSourceName>ComputerStoreDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Kanton">
            <Value>=Parameters!Kanton.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Godina">
            <Value>=Parameters!Godina.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Mjesec">
            <Value>=Parameters!Mjesec.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DS.Naziv,DL.Grad,DP.Podkategorija,SUM(FP.Kolicina) AS [Prodano]
FROM FactProdaja AS FP
JOIN DimSkladista AS DS
ON FP.SkladisteKey=DS.SkladisteKey
JOIN DimLokacije AS DL
ON FP.LokacijaKey=DL.LokacijaKey
JOIN DimProizvodi AS DP
ON FP.ProizvodKey=DP.ProizvodKey
JOIN DimVrijeme AS DV
ON FP.VrijemeKey=DV.VrijemeKey
WHERE (DP.Kategorija LIKE 'Hardver') AND (DL.Kanton=@Kanton) AND(DV.Godina=@Godina)AND(DV.Mjesec=@Mjesec)
GROUP BY  DS.Naziv,DL.Grad,DP.Podkategorija</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Naziv">
          <DataField>Naziv</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Grad">
